@using Planificador.Models.ViewModels;
@model CursadaViewModel
@{
    ViewBag.Title = "Cursada";
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <div>
        <h1> Programador de cursadas </h1>

        <table>
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Materia</td>
                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var m in Model.Materias)
                    {
                        <tr>
                            <td>@m.IdMateria</td>
                            <td>@m.Nombre</td>
                        </tr>
                    }
                }
            </tbody>
        </table>

    </div>
</body>
</html>
